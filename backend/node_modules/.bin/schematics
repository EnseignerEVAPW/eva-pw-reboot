#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules/@angular-devkit/schematics-cli/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules/@angular-devkit/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/node_modules:/home/dotm87/Documentos/Universidad/EVA/node_modules:/home/dotm87/Documentos/Universidad/node_modules:/home/dotm87/Documentos/node_modules:/home/dotm87/node_modules:/home/node_modules:/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules/@angular-devkit/schematics-cli/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules/@angular-devkit/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/node_modules:/home/dotm87/Documentos/Universidad/EVA/node_modules:/home/dotm87/Documentos/Universidad/node_modules:/home/dotm87/Documentos/node_modules:/home/dotm87/node_modules:/home/node_modules:/node_modules:/home/dotm87/Documentos/Universidad/EVA/eva-pw-reboot/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@angular-devkit/schematics-cli/bin/schematics.js" "$@"
else
  exec node  "$basedir/../@angular-devkit/schematics-cli/bin/schematics.js" "$@"
fi
